   "source": [
    "class Solution:\n",
    "  def wordPattern(self, pattern: str, str: str) -> bool:\n",
    "    text = str.split()\n",
    "    return [*map(pattern.index, pattern)] == [*map(text.index, text)]\n",
    "#Time complexity=O(n)\n",
    "#Space complexity=O(n)"
   ]
  }
